					},
					{
						"name": "AverageWeightConfirmed",
						"type": "int",
						"precision": 10
					},
					{
						"name": "TotalWeight",
						"type": "int",
						"precision": 10
					},
					{
						"name": "CreatedAtUTC",
						"type": "datetime2",
						"scale": 7
					},
					{
						"name": "ModifiedAtUTC",
						"type": "datetime2",
						"scale": 7
					},
					{
						"name": "FlockSequence",
						"type": "int",
						"precision": 10
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "Flock"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Producer')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "SqlServer1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "SqlServerTable",
				"schema": [
					{
						"name": "Id",
						"type": "bigint",
						"precision": 19
